{
    "plugins": [
        "theme-api",
        "header",
        "sharing",
        "customJquery@git+https://github.com/dtolb/gitbook-plugin-customJquery.git",
        "toggle-chapters@git+https://github.com/dtolb/gitbook-plugin-toggle-chapters.git"
    ],
    "pluginsConfig": {
        "theme-api": {
            "languages": [
                {
                    "lang": "js",
                    "name": "JavaScript",
                    "default": true
                },
                {
                    "lang": "csharp",
                    "name": "C#"
                },
                {
                    "lang": "ruby",
                    "name": "ruby"

                },
                {
                    "lang": "bash",
                    "name": "cURL"
                }
            ]
        },
      "layout": {
            "headerPath" : "layouts/header.html"
        },
      "customJquery": {
        "js": "js/custom.js"
      },
      "sharing": {
            "facebook": false,
            "twitter": false,
            "google": false,
            "weibo": false,
            "instapaper": false,
            "vk": false,
            "all": [
            "facebook", "google", "twitter",
            "weibo", "instapaper"
        ]
      }
    }
}
